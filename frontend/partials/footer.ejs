<footer style="background-image: url('<%- vars.asset_prefix %><%= cms.siteDetails[0].background.path %>')">
	<div class="inner">
		<div class="top">
			<img src="<%- vars.asset_prefix %><%= cms.siteDetails[0].logo.path %>" alt="<%= cms.siteDetails[0].title %>" />
			<h1>Est. 1972</h1>
			<div class="links"><%- include('links.ejs') %></div>
		</div>
		<div class="bottom">
			<%- include('socials.ejs') %>
			<p>&copy; <%= cms.siteDetails[0].title %> 1972-2024 | <a href="https://dangoweb.com/?from=<%= cms.siteDetails[0].title.toLowerCase().replaceAll(' ', '-') %>">Host</a> | <a href="<%- vars.domain %>/admin">Admin</a> | <%- vars.environment.replace(/\w\S*/g, function(txt) { return txt.charAt(0).toUpperCase() + txt.substr(1).toLowerCase() }) %> Version</p>
		</div>
	</div>
</footer>